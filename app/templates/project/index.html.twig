{% extends 'base.html.twig' %}
{% block content %}
    <p><a href="{{ path('project_create') }}">Create project</a></p>
    <div class="card-columns">
        {% for project in projects %}
            <p><a href="{{ path('project_view', {id: project.id}) }}">{{ project.name }}</a></p>
        {% endfor %}
    </div>
{% endblock %}